<Window x:Class="SquareGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SquareGame"
        mc:Ignorable="d"
        Title="MainWindow" SizeToContent="WidthAndHeight" ContentRendered="Window_ContentRendered" >
    <Grid>
        <Canvas Name="Display" ClipToBounds="True" Height="600" Width="800">
            <Rectangle Name="PlayerRectangle" HorizontalAlignment="Left" Height="50" Width="50" Margin="300,350" Stroke="Black" VerticalAlignment="Top" Fill="Blue" RadiusX="2" RadiusY="2" />
        </Canvas>
    </Grid>
</Window>
